	public function get_new_emp(){
		$where = array('emp_type_id' => 2 , 'emp_status' => 1);
		$count = $this->admin->get_count('employee_tbl', $where) + 1;
		$format = 'NEW-EMP-';
		if($count > 0 && $count < 10){
			$format .= '00' . $count;
		}else if($count >= 10 && $count < 100){
			$format = $format . '0' . $count;
		}else{
			$format = $format . $count;
		}
		$data['result'] = 1;
		$data['info'] = $format;
		
		echo json_encode($data);
	}

